{% extends 'base.html.twig' %}

{% block title %}Fiche contact{% endblock %}

{% block body %}
    <div class="container">
        <h1>Fiche de contact</h1>
        <div class="row justify-content-center mt-5">
            <div class="col-6">
                {% for message in app.flashes('success') %}
                    <div class="alert alert-success" role="alert">
                        {{ message }}
                    </div>
                {% endfor %}

                {{ form_start(form) }}
                {{ form_end(form) }}
            </div>
        </div>
    </div>
{% endblock %}
